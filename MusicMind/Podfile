# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MusicMind' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MusicMind

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'KeychainSwift'
  pod 'Alamofire'
  pod 'SinchVerification-Swift'
  pod 'RecordButton', :git => 'https://github.com/felipericieri/RecordButton.git'

  target 'MusicMindTests' do
      inherit! :search_paths
      pod 'Firebase/Core'
      pod 'Firebase/Auth'
      pod 'Firebase/Storage'
      pod 'Firebase/Database'
      pod 'KeychainSwift'
      pod 'Alamofire'
  end

  target 'MusicMindUITests' do
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    pod 'Firebase/Database'
    pod 'KeychainSwift'
    pod 'Alamofire'

  end


end
